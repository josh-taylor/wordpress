{
  "name": "josh-taylor/wordpress",
  "description": "Composer project for getting started with wordpress",
  "repositories": [
    {
      "type": "package",
      "package": {
	"name": "wordpress",
	"type": "webroot",
	"version": "3.9.1",
	"dist": {
	  "type": "zip",
	  "url": "https://github.com/WordPress/WordPress/archive/3.9.1.zip"
	},
	"require": {
	  "fancyguy/webroot-installer": "1.0.0"
	}
      }
    },
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "wordpress": "3.9.1",
    "fancyguy/webroot-installer": "1.0.0"
  },
  "extra": {
    "webroot-dir": "wp",
    "webroot-package": "wordpress"
  }
}
